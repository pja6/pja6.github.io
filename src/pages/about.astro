---
import BaseLayout from '../layouts/BaseLayout.astro';

const title = 'Paul\'s Resumé';
const description = 'A little bit about me.';
const permalink = `${Astro.site.href}about`;
---

<BaseLayout title={title} description={description} permalink={permalink} current="about">
  <div class="container flex flex-col items-center justify-center">
    <h2>Paul's Resumé</h2>

    <iframe src="    https://docs.google.com/document/d/1uPYoWJqov66yr_JbyXk_agY6WSr8RB8lH00oqRIaq_A/preview
" width="800" height="600"></iframe>


    
</BaseLayout>

<style>
  .about-image {
    float: right;
    margin: -3em -7em 2em 2em;
    max-width: 300px;
  }

  .about-image img {
    border-radius: 8px;
    margin-bottom: 1.5em;
  }

  @media (max-width: 1020px) {
    .about-image {
      float: none;
      margin: 0 auto 2em;
    }
  }
</style>
