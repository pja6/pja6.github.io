---
import BaseLayout from '../layouts/BaseLayout.astro';

//Components

import ProfileMain from '../components/ProfileMain.astro';
import ProfileSideBar from '../components/ProfileSideBar.astro';

const title = 'Paul\'s Space';
const description = 'Paul Aguilar\'s Space';
const permalink = Astro.site.href;
// Your Styles
//import '../styles/MyStyles.css';

---

  


  <!-- Main Header Ends -->
    <BaseLayout title={title} description={description} permalink={permalink}>

    <div class="page-content-container grid grid-cols-2 gap-4">
      <div class="flex pl-10 pr-20">
      <ProfileSideBar />
    </div>
    <div class="flex pl-20 pr-20">
      <ProfileMain />
    </div>
    </div>
	
</BaseLayout>

